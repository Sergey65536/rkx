 #define rk3588
 #include "rk3588-firefly-port-aio-3588jd4.dtsi"
 #include "rk3588-diff-aio-3588jd4.dtsi"



 / {
	position: position {
	        status = "okay";
	        compatible = "firefly,detect-position-gpio";
	        firefly-used-num = <4>;
	        firefly-bit0-gpio = <&gpio1 RK_PD3 GPIO_ACTIVE_HIGH>;
	        firefly-bit1-gpio = <&gpio1 RK_PD0 GPIO_ACTIVE_HIGH>;
	        firefly-bit2-gpio = <&gpio1 RK_PD2 GPIO_ACTIVE_HIGH>;
	        firefly-bit3-gpio = <&gpio1 RK_PD1 GPIO_ACTIVE_HIGH>;
	};
/* led */
	firefly_leds: leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 =<&led_power &led_user>;
		status = "okay";
		power_led: power {
			label = ":power_green"; //green led
                        linux,default-trigger = "ir-power-click";
			gpios = <&gpio1 RK_PA1 GPIO_ACTIVE_HIGH>;//L led
			default-state = "on";
		};

		user_led: user {
			label = ":user_red"; //red led
                        linux,default-trigger = "ir-user-click";
			gpios = <&gpio1 RK_PA0 GPIO_ACTIVE_HIGH>; //R led
			default-state = "off";
		};
	};
};

&fiq_debugger {
	rockchip,baudrate = <115200>;  /* Only 115200 and 1500000 */
};

&i2c0 {
	pinctrl-0 = <&i2c0m2_xfer>;
	status = "okay";
	core_pc9202: pc9202@3c {
		status = "okay";
		compatible = "firefly,pc9202";
		reg = <0x3c>;
		pinctrl-names = "default";
		pinctrl-0 = <&core_pc9202_en_pin>;
		wd-en-gpio = <&gpio2 RK_PC5 GPIO_ACTIVE_HIGH>;
		driver-names = "wdt_core";
	};
};

&i2c3 {
	pinctrl-0 = <&i2c3m2_xfer>;
	status = "okay";
	base_pc9202: pc9202@3c {
		status = "okay";
		compatible = "firefly,pc9202";
		reg = <0x3c>;
		pinctrl-names = "default";
		pinctrl-0 = <&base_pc9202_en_pin>;
		wd-en-gpio = <&gpio3 RK_PC4 GPIO_ACTIVE_HIGH>;
	};
};

/* gamc0 */
&gmac0 {
	snps,reset-gpio = <&gpio4 RK_PA2 GPIO_ACTIVE_LOW>;
	//tx_delay = <0x35>;
	status = "okay";
};

&gmac0_tx_bus2{
    rockchip,pins =
    /* gmac0_txd0 */
    <2 RK_PB6 1 &pcfg_pull_up_drv_level_6>,
    /* gmac0_txd1 */
    <2 RK_PB7 1 &pcfg_pull_up_drv_level_6>,
    /* gmac0_txen */
    <2 RK_PC0 1 &pcfg_pull_none>;
};

&gmac0_rgmii_bus{
	rockchip,pins =
	/* gmac0_rxd2 */
	<2 RK_PA6 1 &pcfg_pull_none>,
	/* gmac0_rxd3 */
	<2 RK_PA7 1 &pcfg_pull_none>,
	/* gmac0_txd2 */
	<2 RK_PB1 1 &pcfg_pull_up_drv_level_6>,
	/* gmac0_txd3 */
	<2 RK_PB2 1 &pcfg_pull_up_drv_level_6>;
};

/* gmac1 */
&gmac1 {
	snps,reset-gpio = <&gpio4 RK_PB5 GPIO_ACTIVE_LOW>;
	tx_delay = <0x45>;
	status = "okay";
};

&gmac1_tx_bus2{
	rockchip,pins =
	/* gmac1_txd0 */
	<3 RK_PB3 1 &pcfg_pull_up_drv_level_6>,
    /* gmac1_txd1 */
	<3 RK_PB4 1 &pcfg_pull_up_drv_level_6>,
	/* gmac1_txen */
	<3 RK_PB5 1 &pcfg_pull_none>;
};

&gmac1_rgmii_bus{
	rockchip,pins =
	/* gmac1_rxd2 */
	<3 RK_PA2 1 &pcfg_pull_none>,
	/* gmac1_rxd3 */
	<3 RK_PA3 1 &pcfg_pull_none>,
	/* gmac1_txd2 */
	<3 RK_PA0 1 &pcfg_pull_up_drv_level_6>,
	/* gmac1_txd3 */
	<3 RK_PA1 1 &pcfg_pull_up_drv_level_6>;
};


//down
&usbdrd_dwc3_0{
	//dr_mode = "host";
	dr_mode = "peripheral";
	status = "okay";
};

//down
&usbdrd_dwc3_1{
	dr_mode = "peripheral";
	status = "okay";
};

&combphy0_ps{
	status = "okay";
};


/* pcie3.0 x 4 slot */
&pcie30phy {
        status = "okay";
};

&pcie3x4 {
        num-lanes = <2>;
        reset-gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
        vpcie3v3-supply = <&vcc3v3_pcie30>;
        status = "okay";
};

&vcc3v3_pcie30{
        gpios = <&gpio0 RK_PD3 GPIO_ACTIVE_HIGH>;
        startup-delay-us = <5000>;
        status = "okay";
};

/* pinctrl */
&pinctrl {

	leds {
		led_power: led-power {
			/* led_power */
			rockchip,pins = <1 RK_PA1 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		led_user: led-user {
			/* led_user */
			rockchip,pins = <1 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wdt-en {
		core_pc9202_en_pin: core-pc9202-en-pin {
			rockchip,pins = <2 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		base_pc9202_en_pin: base-pc9202-en-pin {
			rockchip,pins = <3 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
        };

	headphone {
		hp_det: hp-det {
			rockchip,pins = <2 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb-typec {
		usbc0_int: usbc0-int {
			rockchip,pins = <0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	hdmirx {
		hdmirx_det: hdmirx-det {
			rockchip,pins = <1 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-bluetooth {
		uart6_gpios: uart6-gpios {
			rockchip,pins = <1 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_reset_gpio: bt-reset-gpio {
			rockchip,pins = <0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_wake_gpio: bt-wake-gpio {
			rockchip,pins = <0 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_irq_gpio: bt-irq-gpio {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-wlan {
		wifi_host_wake_irq: wifi-host-wake-irq {
			rockchip,pins = <0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		wifi_poweren_gpio: wifi-poweren-gpio {
			rockchip,pins = <0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sata {
		sata_reset:sata-reset{
			rockchip,pins = <4 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

};
