 #define rk3588
 #include "rk3588-firefly-port-aio-3588jd4.dtsi"
 #include "rk3588-diff-aio-3588jd4.dtsi"


#define M2_SATA_OR_PCIE	1 /*1 = SATA , 0 = PCIe */

 / {
/* led */
	firefly_leds: leds {
		power_led: power {
			status = "disabled";

			gpios = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;//green led
            default-state = "on";
		};

		user_led: user {
			gpios = <&pca9555 PCA_IO0_3 GPIO_ACTIVE_HIGH>; //yellow led
            default-state = "off";
		};
	};

    fan: pwm-fan {
		compatible = "pwm-fan";
		#cooling-cells = <2>;
		fan-supply = <&vcc5v0_sys>;
		pwms = <&pwm5 0 50000 0>;
		cooling-levels = <50 50 100 150 200 255>;
        rockchip,temp-trips = <
            50000   1
            55000   2
            60000   3
            65000   4
            70000   5
        >;
	};
   
    mod_sleep: mod-sleep-regulator {
            compatible = "regulator-fixed";
            regulator-name = "mod_sleep";
            regulator-boot-on;
            regulator-always-on;
            enable-active-high;
            status = "okay";
	    gpio = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;//green led
	    pinctrl-names = "default";
	    pinctrl-0 =<&leds_gpio>;
		regulator-state-mem {
			regulator-on-in-suspend;
		};
    };

    vcc_hub: vcc-hub-regulator {
            compatible = "regulator-fixed";
            regulator-name = "vcc_hub";
            regulator-boot-on;
            regulator-always-on;
            enable-active-high;
            status = "okay";
            gpio = <&pca9555 PCA_IO0_1 GPIO_ACTIVE_HIGH>;
    };

	vcc_4g: vcc-4g-regulator {
            compatible = "regulator-fixed";
            regulator-name = "vcc_4g";
            regulator-boot-on;
            regulator-always-on;
            enable-active-high;
            status = "okay";
            gpio = <&pca9555 PCA_IO0_0 GPIO_ACTIVE_HIGH>;
    };

    usb_5v: usb-5v {
       	compatible = "regulator-fixed";
            regulator-name = "usb_5v";
            regulator-boot-on;
            regulator-always-on;
            enable-active-high;
            status = "okay";
            gpio = <&gpio1 RK_PA3 GPIO_ACTIVE_HIGH>;
	    pinctrl-names = "default";
	    pinctrl-0 =<&usb_5v_ctrl>;
    };
};

/* gamc0 */
&gmac0 {
	snps,reset-gpio = <&gpio4 RK_PA2 GPIO_ACTIVE_LOW>;
	tx_delay = <0x31>;
	status = "okay";
};

&gmac0_tx_bus2{
    rockchip,pins =
    /* gmac0_txd0 */
    <2 RK_PB6 1 &pcfg_pull_up_drv_level_6>,
    /* gmac0_txd1 */
    <2 RK_PB7 1 &pcfg_pull_up_drv_level_6>,
    /* gmac0_txen */
    <2 RK_PC0 1 &pcfg_pull_none>;
};

&gmac0_rgmii_bus{
	rockchip,pins =
	/* gmac0_rxd2 */
	<2 RK_PA6 1 &pcfg_pull_none>,
	/* gmac0_rxd3 */
	<2 RK_PA7 1 &pcfg_pull_none>,
	/* gmac0_txd2 */
	<2 RK_PB1 1 &pcfg_pull_up_drv_level_6>,
	/* gmac0_txd3 */
	<2 RK_PB2 1 &pcfg_pull_up_drv_level_6>;
};

/* gmac1 */
&gmac1 {
	snps,reset-gpio = <&gpio4 RK_PB0 GPIO_ACTIVE_LOW>;
	tx_delay = <0x40>;
	status = "okay";
};

&gmac1_tx_bus2{
	rockchip,pins =
	/* gmac1_txd0 */
	<3 RK_PB3 1 &pcfg_pull_up_drv_level_6>,
    /* gmac1_txd1 */
	<3 RK_PB4 1 &pcfg_pull_up_drv_level_6>,
	/* gmac1_txen */
	<3 RK_PB5 1 &pcfg_pull_none>;
};

&gmac1_rgmii_bus{
	rockchip,pins =
	/* gmac1_rxd2 */
	<3 RK_PA2 1 &pcfg_pull_none>,
	/* gmac1_rxd3 */
	<3 RK_PA3 1 &pcfg_pull_none>,
	/* gmac1_txd2 */
	<3 RK_PA0 1 &pcfg_pull_up_drv_level_6>,
	/* gmac1_txd3 */
	<3 RK_PA1 1 &pcfg_pull_up_drv_level_6>;
};


&pwm5 {
	pinctrl-0 = <&pwm5m1_pins>;
	status = "okay";
};


/* tf-card */
&sdmmc {
	status = "okay";
};

&vcc_sdcard_pwr_en{
	gpio = <&gpio1 RK_PA7 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

/* usb */
&vcc5v0_host{
	status = "okay";
	gpio = <&pca9555 PCA_IO0_2 GPIO_ACTIVE_HIGH>;
};


//down
&usbdrd_dwc3_0{
	dr_mode = "host";
	//dr_mode = "peripheral";
    status = "okay";
};

//down
&usbdrd_dwc3_1{
	dr_mode = "host";
    status = "okay";
};

/* XL9555*/

&i2c6 {
	/delete-node/ pca9555;
};

/* wdt on mb */
&i2c4 {
	status = "okay";
	pinctrl-0 = <&i2c4m1_xfer>;
	pc9202@3c {
		status = "okay";
		compatible = "firefly,pc9202";
		reg = <0x3c>;
		index = <1>;
		pinctrl-names = "default";
		pinctrl-0 = <&wdt_en_base>;

		wd-en-gpio = <&gpio0 RK_PC4 GPIO_ACTIVE_HIGH>;
		driver-names = "wdt_base";
	};
};

#if M2_SATA_OR_PCIE
&sata0 {
    //pinctrl-names = "default";
    //pinctrl-0 = <&sata_reset>;
	status = "okay";
};
#else
&pcie2x1l2 {
	reset-gpios = <&gpio4 RK_PA3 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc_sata_pwr_en>;
	status = "okay";
};
#endif

&vcc_sata_pwr_en{
	status = "okay";
	gpio = <&pca9555 PCA_IO1_4 GPIO_ACTIVE_HIGH>;  //PCA_IO 14
	regulator-min-microvolt = <3300000>;
	regulator-max-microvolt = <3300000>;
	regulator-always-on;
	enable-active-high;
	startup-delay-us = <5000>;
	vin-supply = <&vcc12v_dcin>;
};

&combphy0_ps{
	status = "okay";
};

/* can1 */
&can1 {
	status = "okay";
	assigned-clocks = <&cru CLK_CAN1>;
	assigned-clock-rates = <200000000>;
	pinctrl-names = "default";
	pinctrl-0 = <&can1m1_pins>;
};

/* RS485 */
&uart6{
	pinctrl-0 = <&uart6m1_xfer>;
	status = "okay";
};

/* RS232 */
&uart1{
	pinctrl-0 = <&uart1m1_xfer>;
	status = "okay";
};


/* hdmi0 */
&hdmi0 {
    enable-gpios = <&gpio1 RK_PB0 GPIO_ACTIVE_HIGH>;
    status = "okay";
};


&hdmi0_sound {
	status = "okay";
};

&hdmi0_in_vp0 {
        status = "okay";
};

&hdptxphy_hdmi0 {
	status = "okay";
};

&route_hdmi0{
	status = "okay";
	connect = <&vp0_out_hdmi0>;
};

/* es8323 audio codec */

&i2c6 {
		status = "okay";
		es8388: es8388@11 {
			status = "okay";
			#sound-dai-cells = <0>;
			compatible = "everest,es8388", "everest,es8323";
			reg = <0x11>;
			clocks = <&mclkout_i2s0>;
			clock-names = "mclk";
			assigned-clocks = <&mclkout_i2s0>;
			assigned-clock-rates = <12288000>;
			pinctrl-names = "default";
			pinctrl-0 = <&i2s0_mclk>;
 	};
};

&es8388_sound{
	status = "okay";
	firefly,not-use-dapm;
	/delete-property/ spk-con-gpio;
	hp-det-gpio = <&gpio2 RK_PC3 GPIO_ACTIVE_HIGH>;
	hp-con-gpio = <&pca9555 PCA_IO1_3 GPIO_ACTIVE_HIGH>;  //PCA_IO 13
	pinctrl-names = "default";
	pinctrl-0 = <&hp_det>;
        /delete-property/ io-channels;
        /delete-property/ io-channel-names;
        rockchip,audio-routing =
                        "Headphone", "LOUT1",
                        "Headphone", "ROUT1",
                        "Speaker", "LOUT2",
                        "Speaker", "ROUT2",
                        "Headphone", "Headphone Power",
                        "Headphone", "Headphone Power",
                        "LINPUT2", "Main Mic",
                        "RINPUT2", "Main Mic",
                        "LINPUT1", "Headset Mic",
                        "RINPUT1", "Headset Mic";
        linein-type = <1>;
        pinctrl-names = "default";

};

&i2s0_8ch{
	status = "okay";
};



/* wifi*/

&pcie30phy {
	status = "okay";
};


&pcie3x4 {
	num-lanes = <1>;
	reset-gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie30>;
	status = "okay";
};

&vcc3v3_pcie30{
	gpios = <&pca9555 PCA_IO0_4 GPIO_ACTIVE_HIGH>;
	startup-delay-us = <5000>;
	status = "okay";
};

&wireless_wlan{
        wifi_chip_type = "rtl8822ce";
		/delete-property/ WIFI,poweren_gpio;
		WIFI,host_wake_irq = <&pca9555 PCA_IO1_2 GPIO_ACTIVE_HIGH>;
        status = "okay";
};

//debug 115200
&fiq_debugger{
	rockchip,baudrate = <115200>;  /* Only 115200 and 1500000 */
};

/* pinctrl */
&pinctrl {

    leds {
		leds_gpio: leds-gpio {
			/* led_power */
			rockchip,pins = <0 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
 	};


	headphone {
		hp_det: hp-det {
			rockchip,pins = <2 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb-typec {
		usb_5v_ctrl: usb-5v-ctrl {
			rockchip,pins = <1 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		usbc0_int: usbc0-int {
			rockchip,pins = <0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	hdmirx {
		hdmirx_det: hdmirx-det {
			rockchip,pins = <1 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-bluetooth {
		uart6_gpios: uart6-gpios {
			rockchip,pins = <1 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_reset_gpio: bt-reset-gpio {
			rockchip,pins = <0 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_wake_gpio: bt-wake-gpio {
			rockchip,pins = <0 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		bt_irq_gpio: bt-irq-gpio {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-wlan {
		wifi_host_wake_irq: wifi-host-wake-irq {
			rockchip,pins = <0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		};

	wdt-pc9202 {
		
		wdt_en_base: wdt-en-base {
			rockchip,pins = <0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
		};

	};

	sata {
		sata_reset:sata-reset{
			rockchip,pins = <4 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

};
