/ {

	vcc3v3_tp_en_regulator: vcc3v3-tp-en-regultor{
		status = "okay";
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_tp_en";
		regulator-always-on;
		enable-active-high;
        gpio = <&pca9555 PCA_IO0_6 GPIO_ACTIVE_HIGH>;
